 @model IEnumerable<Employee>
<div class = 'container'>
    <table class = "table">
        <thead>
            <th>@Html.DisplayNameFor(d=>d.EmpId)</th>
            <th>@Html.DisplayNameFor(d=>d.Name)</th>
            <th>@Html.DisplayNameFor(d=>d.Salary)</th>
            <th>@Html.DisplayNameFor(d=>d.Email)</th>
            <th>@Html.DisplayNameFor(d=>d.Dob)</th>
        </thead>
        <tbody>
            @foreach(var data in @Model)
            {
                <tr>
                    <td>@Html.DisplayFor(e=>data.EmpId)</td>
                    <td>@Html.DisplayFor(e=>data.Name)</td>
                    <td>@Html.DisplayFor(e=>data.Salary)</td>
                    <td>@Html.DisplayFor(e=>data.Email)</td>
                    <td>@Html.DisplayFor(e=>data.Dob)</td>
                </tr>
            }
        </tbody>
    </table>
</div>
